{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "/",
      "destination": "/main.html"
    }, {
      "source": "/favicon.ico",
      "destination": "/img/favicon.ico"
    } ],
    "cleanUrls": true
  }
}
